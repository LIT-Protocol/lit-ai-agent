{
  "name": "lit-agent-contracts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lit-agent-contracts",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "forge build",
        "cwd": "packages/lit-agent-contracts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "forge test",
        "cwd": "packages/lit-agent-contracts"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "forge clean && rm -rf {projectRoot}/cache {projectRoot}/out {projectRoot}/node_modules"
      }
    }
  },
  "tags": []
}
